allow smbd_t container_file_t:file read;

#!!!! This avc is allowed in the current policy
allow smbd_t default_t:dir { add_name create remove_name rename setattr write };

#!!!! This avc is allowed in the current policy
allow smbd_t default_t:file { getattr read };

#!!!! This avc is allowed in the current policy
allow smbd_t unlabeled_t:dir { add_name create open read remove_name rename search setattr write };

#!!!! This avc is allowed in the current policy
allow smbd_t unlabeled_t:file { create getattr open read setattr unlink write };
