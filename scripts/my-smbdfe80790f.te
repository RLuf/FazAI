
module my-smbdfe80790f 1.0;

require {
	type unlabeled_t;
	type smbd_t;
	type container_file_t;
	type default_t;
	class dir { add_name create open read remove_name rename search setattr write };
	class file { create getattr open read setattr unlink write };
}

#============= smbd_t ==============

