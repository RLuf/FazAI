# GenAI Mini Framework - Configuração de Ambiente
# Copie este arquivo para .env e configure as variáveis

# Google GenAI API (OBRIGATÓRIO)
GOOGLE_API_KEY=

# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333

# Cache Configuration  
CACHE_DB_FILE=genai_cache.db

# Llama.cpp Servers (OPCIONAL - funcionará apenas com GenAI se não configurado)
LLAMA_GERENTE_URL=http://localhost:8000/v1
LLAMA_ANALISTA_URL=http://localhost:8001/v1
LLAMA_PROGRAMADOR_URL=http://localhost:8002/v1

# Framework Settings
MAX_STEPS=30
TIMEOUT_SECONDS=30
LOG_LEVEL=INFO
ENABLE_CACHE=true
ENABLE_FALLBACK=true
